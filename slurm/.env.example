# 配置文件示例

# 服务配置
export PORT=8080
export GIN_MODE=release

# SSH 连接默认配置（可选）
export DEFAULT_SSH_PORT=22
export DEFAULT_SSH_TIMEOUT=30

# 日志配置
export LOG_LEVEL=info
export LOG_FILE=slurm-jobacct.log

# 示例 SSH 连接配置
# 注意：不要在配置文件中存储敏感信息如密码
export SLURM_HOST=your-slurm-login-node
export SLURM_USER=your-username
# export SLURM_PRIVATE_KEY=/path/to/your/private/key
